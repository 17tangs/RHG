import os

directory = os.fsencode("C:\Users\gf174cq\rhg-xsd\xsdGenerator\typeFiles")

for file in os.listdir(directory):
    filename = os.fsdecode(file)
    if filename.endswith(".xsd"):
        print(filename)
        # print(os.path.join(directory, filename))
        continue
    else:
        continue
